    ]
  },
  "mixins": [
    "images.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.7.1",
    "fabric": ">=0.11.8",
    "fabric-language-kotlin": ">=1.3.71",
    "minecraft": ">=1.16.1"
  }
}
