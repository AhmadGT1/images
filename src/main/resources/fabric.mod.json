  "name": "Images",
  "description": "Adds picture frames for displaying imgur images in game",
  "authors": [
    "Sorenon"
  ],
  "icon": "assets/images/pack.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.sorenon.images.init.ImagesMod"
